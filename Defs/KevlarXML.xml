<Defs>


    <ThingDef ParentName="ResourceBase">
        <defName>KevlarMacialKek</defName>
        <label>Magical Kevlar</label>
        <description>Kevlar with much better armor characteristics.</description>
        <graphicData>
            <texPath>Things/Item/Resource/Cloth</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
            <color>(70, 0, 70)</color>
        </graphicData>
        <statBases>
            <Bulk>0.05</Bulk>
			<WornBulk>0.5</WornBulk>
