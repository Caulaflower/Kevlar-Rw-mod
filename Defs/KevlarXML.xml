        </statBases>
        <burnableByRecipe>true</burnableByRecipe>
        <healthAffectsPrice>false</healthAffectsPrice>
        <minRewardCount>30</minRewardCount>
        <stuffProps>
        <categories>
            <li>Fabric</li>
            <li>SoftArmor</li>
        </categories>
        <commonality>0.10</commonality>
        <color>(70, 0, 70)</color>
        <statFactors>
            <Flammability>0</Flammability>
            <MaxHitPoints>2.4</MaxHitPoints>
            <Beauty>2.5</Beauty>
        </statFactors>
        </stuffProps>
        <thingCategories>
        <li>Textiles</li>
        </thingCategories>
        <tradeTags>
        <li>ExoticMisc</li>
        </tradeTags>


        <costList>
            <Cloth>10</Cloth>
            <Chemfuel>30</Chemfuel>
          </costList>
          <recipeMaker>
            <workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
            <workSkill>Intellectual</workSkill>
            <recipeUsers>
              <li>HandTailoringBench</li>
              <li>ElectricTailoringBench</li>
            </recipeUsers>
            <skillRequirements>
              <Intellectual>8</Intellectual>
            </skillRequirements>
          </recipeMaker>
    </ThingDef>

